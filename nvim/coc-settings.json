{
    "python.jediEnabled": false,
    "python.linting.pylintEnabled": false,
    "python.formatting.provider": "black",
    "python.venvFolders": [
      ".venv"
    ],
    "coc.preferences.formatOnSaveFiletypes": ["python", "typescript", "typescriptreact", "rust", "xml", "javascript", "javascriptreact"],
    "coc.preferences.rootPatterns": [
      "manage.py",
      ".git",
      ".hg",
      ".projections.json"
    ],
    "eslint.autoFixOnSave": true,
    "list.source.files.excludePatterns": [
	    "**/__pycache__/*",
	    "**/media/*",
	    "**/.cache/*",
	    "**/.git/*",
	    "**/.pytest_cache/*",
	    "**/.mypy_cache/*",
	    "**/node_modules/*",
	    "**/node_modules/**",
	    "**/node_modules/**/.*",
	    "**/node_modules/**/.*/**",
	    "**/.venv/*",
	    "**/.venv/**",
	    "**/.venv/**/.*",
	    "**/.venv/**/.*/**",
	    "**/.git/*",
	    "**/.git/**",
	    "**/.git/**/.*",
	    "**/.git/**/.*/**"
    ],
    "suggest.triggerAfterInsertEnter": true,
    "suggest.noselect": false,
    "coc.source.around.enable": false,
    "coc.source.buffer.enable": false,
    "list.source.files.args": ["--hidden", "--files"],
    "typescript.format.enabled": false,
    "typescript.suggest.completeFunctionCalls": false,
    "javascript.format.enabled": false,
    "explorer.icon.enableNerdfont": true,
    "explorer.file.hiddenRules": {
      "extensions": [
        "o",
        "a",
        "obj",
        "pyc"
      ],
      "filenames": [],
      "patternMatches": [
        "^\\.",
        "lib",
        "node_modules",
        "^build\/",
        "__pycache__"
      ]
    },
    "diagnostic.checkCurrentLine": true,
    "list.insertMappings": {
      "<C-j>": "normal:j",
      "<C-k>": "normal:k",
      "<C-n>": "normal:j"
    },
    "list.normalMappings": {
      "<C-j>": "normal:j",
      "<C-k>": "normal:k",
      "<C-n>": "normal:j"
    },
    "list.nextKeymap": "<C-n>",
    "list.previousKeymap": "<C-p>"
}

